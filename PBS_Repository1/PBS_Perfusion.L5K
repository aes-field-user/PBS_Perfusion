			                        DefaultData := 1);
			Cfg_HasMOoS : BOOL (Description := "1=Maintenance Out of Service exists, can be selected",
			                       Usage := Input,
			                       RADIX := Decimal,
			                       Required := No,
			                       Visible := No,
			                       DefaultData := 1);
			Cfg_OvrdOverLock : BOOL (Description := "1=Override supersedes Prog/Oper Lock, 0=don$'t override Lock",
			                            Usage := Input,
			                            RADIX := Decimal,
